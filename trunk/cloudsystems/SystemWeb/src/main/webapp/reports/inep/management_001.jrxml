	<queryString>
		<![CDATA[SELECT
	EVT.*,
	CLI_INTERNAL_CODE_CH CODIGO,
	USR_NAME_CH POSTO,
	( SELECT coalesce ( COUNT(*), 0 ) FROM INEP.INEP_SUBSCRIPTION SB WHERE SB.USR_ID_IN = S.USR_ID_IN AND S.CLI_SEQ_IN = SB.CLI_SEQ_IN ) INSCRICOES,
	( SELECT coalesce ( COUNT(*), 0 ) FROM INEP.INEP_SUBSCRIPTION SB WHERE SB.USR_ID_IN = S.USR_ID_IN AND S.CLI_SEQ_IN = SB.CLI_SEQ_IN AND ISS_ID_IN IN (1, 3 ) ) AUSENTES,
	( SELECT coalesce ( COUNT(*), 0 ) FROM INEP.INEP_SUBSCRIPTION SB INNER JOIN INEP.INEP_MEDIA IM ON ( SB.USR_ID_IN = IM.USR_ID_IN AND SB.PCT_ID_IN = IM.PCT_ID_IN AND SB.ISC_ID_CH = IM.ISC_ID_CH AND IMT_ID_IN = 2 )
	WHERE SB.USR_ID_IN = S.USR_ID_IN AND S.CLI_SEQ_IN = SB.CLI_SEQ_IN AND ISS_ID_IN IN (2) ) AUDIOS
FROM
	INEP.INEP_STATION S INNER JOIN CLIENT C ON ( S.CLI_SEQ_IN = C.CLI_SEQ_IN AND S.USR_ID_IN = C.USR_ID_IN )
	INNER JOIN USERS U ON ( C.CLI_ID_IN = U.USR_ID_IN )
	INNER JOIN INEP.INEP_PACKAGE EVT ON ( S.USR_ID_IN = EVT.USR_ID_IN AND S.PCT_ID_IN = EVT.PCT_ID_IN )
where s.usr_id_in = $P{COMPANY_ID}
and s.pct_id_in = $P{EVENT_ID}
ORDER BY EVT.USR_ID_IN, EVT.PCT_ID_IN, USR_NAME_CH]]>
	</queryString>
