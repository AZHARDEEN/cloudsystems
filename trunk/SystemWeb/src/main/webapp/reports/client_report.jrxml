	<queryString>
		<![CDATA[SELECT DISTINCT
	U.USR_NAME_CH AS NAME,
	(SELECT UD.UDC_CODE_CH FROM USER_DOCUMENT UD WHERE UD.USR_ID_IN = U.USR_ID_IN AND DOC_ID_IN = 6) AS EMAIL,
	D.UDC_CODE_CH CPF	
FROM 
	CLIENT C INNER JOIN PERSON P ON ( C.CLI_ID_IN = P.USR_ID_IN )
		INNER JOIN USERS U ON ( P.USR_ID_IN = U.USR_ID_IN )
			LEFT OUTER JOIN USER_DOCUMENT D ON ( U.USR_ID_IN = D.USR_ID_IN AND DOC_ID_IN = 1)
WHERE
	C.USR_ID_IN =  $P{COMPANY_ID}
ORDER BY U.USR_NAME_CH asc]]>
	</queryString>
