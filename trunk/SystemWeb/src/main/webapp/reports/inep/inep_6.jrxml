	<queryString>
		<![CDATA[SELECT
	s.isc_id_ch subscription,
	s.isc_written_grade_nm written,
	coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm ) oral,
	case when s.isc_written_grade_nm < coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm )
	then s.isc_written_grade_nm else coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm ) end as final
FROM
	INEP.INEP_SUBSCRIPTION s LEFT OUTER JOIN INEP.INEP_ORAL_test o on (
		s.usr_id_in = o.usr_id_in and s.pct_id_in = o.pct_id_in
		and s.isc_id_ch = o.isc_id_ch )
WHERE
  s.usr_id_in = $P{COMPANY_ID} AND
  s.pct_id_in = $P{EVENT_ID}
ORDER BY s.isc_id_ch]]>
	</queryString>
