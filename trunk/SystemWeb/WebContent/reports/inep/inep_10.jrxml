	<queryString>
		<![CDATA[select
	T.isc_id_ch,
	iot_final_grade_nm posto,
	iot_agreement_grade_in revisao,
	iot_agreement2_grade_nm consenso,
	coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm ) PO,
	isc_written_grade_nm PE,
	abs ( Coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm ) - isc_written_grade_nm ) DISCREPANCIA,
	case when isc_written_grade_nm <  coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm )
	then isc_written_grade_nm else coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm ) end NF
from
	inep.inep_oral_test t,
	inep.inep_subscription s
where iot_variance_in >= 10
and s.isc_id_ch = t.isc_id_ch
and t.usr_id_in = $P{COMPANY_ID}
and t.pct_id_in = $P{EVENT_ID}
order by
	abs ( Coalesce ( iot_agreement2_grade_nm, iot_agreement_grade_in, iot_final_grade_nm ) - isc_written_grade_nm ) desc]]>
	</queryString>
